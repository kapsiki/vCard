{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "~~/*": ["./*"],
      "@@/*": ["./*"]
    },
    "module": "es2015",
      "moduleResolution": "node",
      "target": "es5",
      "sourceMap": true,
      "allowJs": true
  },
  "exclude": ["node_modules", ".nuxt", "dist"],
  "include": [
      "./*"
    ]
}
